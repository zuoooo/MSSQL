SELECT * FROM BC200T
SELECT * FROM BC100T

UPDATE BC200T 
SET LEVEL3 = 'BR04' 
WHERE ITEM_CD = 'B210TBS-HD'

SELECT ITEM_CD, ITEM_NM, ISNULL(NULLIF(MATERIAL, ''), 'A')
FROM BC200T
WHERE  ITEM_CD IN( 'B230LNA-07', 'B210TBS-09' , 'B210TBS-HD')
ORDER BY ITEM_CD DESC

SELECT ITEM_CD, ITEM_NM,
CASE
WHEN MATERIAL = '' THEN 'A'
END
FROM BC200T
WHERE  ITEM_CD IN( 'B230LNA-07', 'B210TBS-09' , 'B210TBS-HD')
ORDER BY ITEM_CD DESC
